<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- <inherits name="com.google.gwt.core.Core" /> -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />

	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.gdx.physics.box2d.box2d-gwt' />
	<inherits name='com.badlogic.gdx.pay' />
	<inherits name='aurelienribon.tweenengine' />
	<inherits name='Entry' />
	<inherits name='gcreate' />

	<entry-point class='com.gcreate.suduku.client.HtmlLauncher' />

	<set-configuration-property name="gdx.assetpath" value="../android/assets" />

	<extend-configuration-property name="gdx.reflect.include" value="com.gcreate.model" />
	<extend-configuration-property name="gdx.reflect.include" value="com.gcreate.temp" />
	<extend-configuration-property name="gdx.reflect.include" value="com.gcreate.user" />
	<extend-configuration-property name="gdx.reflect.include" value="com.gcreate.mathcalc" />
	<extend-configuration-property name="gdx.reflect.include" value="viewHandler" />

	<set-configuration-property name="gdx.assetpath" value="../android/assets" />

	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe" />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE' />

	<!-- release using this configure <add-linker name="xs" /> -->

</module>
